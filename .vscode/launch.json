{
  "configurations": [
    {
      "args": [
        "run",
        "--all-files"
      ],
      "module": "pre_commit",
      "name": "pre-commit_run_all",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "commit",
        "--all",
        "--",
        "-s"
      ],
      "console": "externalTerminal",
      "module": "commitizen",
      "name": "commit_to_repo",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "pull_from_repo"
      ],
      "console": "externalTerminal",
      "module": "py_project_dev_tools",
      "name": "pull_from_repo",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "push_to_repo"
      ],
      "console": "externalTerminal",
      "module": "py_project_dev_tools",
      "name": "push_to_repo",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "serve"
      ],
      "module": "mkdocs",
      "name": "mkdocs_serve",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "build"
      ],
      "module": "mkdocs",
      "name": "mkdocs_build",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "make_exe_release",
        "--project_toml_path",
        "${workspaceFolder}/pyproject.toml"
      ],
      "module": "py_project_dev_tools",
      "name": "make_exe_release",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [],
      "module": "ue4ss_installer",
      "name": "ue4ss_installer",
      "request": "launch",
      "type": "debugpy"
    }
  ],
  "version": "0.2.0"
}
